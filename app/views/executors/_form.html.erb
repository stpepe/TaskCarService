<%= form_with model: @executor do |executor|%>
    <div class="mb-3 row mt-3">
        <%= executor.label :name, 'Имя', class: 'col-sm-2 col-form-label' %>
        <div class="col-sm-10">
            <%= executor.text_field :name, class: 'form-control' %>
        </div>
    </div>
    <div class="mb-3 row mt-3">
        <%= executor.label :name, 'Фамилия', class: 'col-sm-2 col-form-label' %>
        <div class="col-sm-10">
            <%= executor.text_field :second_name, class: 'form-control' %>
        </div>
    </div>
    <%= executor.submit "Отправить", class: 'btn btn-primary'%>
<% end %>